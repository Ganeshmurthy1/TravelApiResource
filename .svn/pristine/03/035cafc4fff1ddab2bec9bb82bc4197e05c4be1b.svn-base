package com.tayyarah.flight.exception;

import com.tayyarah.common.exception.BaseException;

public class FlightAPIResponseintializeException  extends BaseException {
	private static final long serialVersionUID = 8823356956727033191L;
	
	private String msg;
	public FlightAPIResponseintializeException(String msg) {
		this.msg = msg;
	}
	@Override
	public String getMessage() {
		return msg;
	}
	@Override
	public String toString() {
		return "ControllerException"+msg;
	}

}
