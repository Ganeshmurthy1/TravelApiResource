<meta name="viewport" content="width=device-width,initial-scale=1" />
 <style type="text/css">

     body {
            margin: 0;
                padding: 0;
                min-width: 100%;
                background-color: #f1f1f1;
            }
            table {
                border-spacing: 0;
                font-family: sans-serif;
                color: #333333;
            }
            td {
                padding: 0;
                font-size: 13px;
                    text-align: center;
            }
            img {
                border: 0;
            }
            .wrapper {
                width: 100%;
                table-layout: fixed;
                -webkit-text-size-adjust: 100%;
                -ms-text-size-adjust: 100%;
            }
            .webkit {
                max-width: 600px;
                margin: 0 auto;
            }
            .outer {
                margin: 10px auto;
                    width: 100%;
                    max-width: 600px;                    
                    background-color: #f9f9f9;    
                    border: 1px solid #ddd;
                }
                .text-left{
                    text-align: left;
                }
                .text-right{
                    text-align: right;
                }
                .inner {
                width: 40%               
                 }
                 .inner img{
                    padding: 10px;
                    width: 80%;
                 }
                 .in-no-date{
                    width: 60%        
                 }

                 .main-head{
                    background-color: #000;
                    
                }


                .invoicefrom-address{
                    width: 60%;
                }
                .invoicefrom-address h4{
                        margin: 4px;
                }
                .invoice-no p{
                        font-size: 13px;
                        font-family: Georgia, Times, serif;
                        line-height: 0px;  
                        color: #fff;              
                }
                .invoice-no .h2no p{
                    font-size: 18px;
                }
                .to-from{
                    background-color: #f1f1f1;
                }
                .to-from h2{
                    background-color: #ccc;
                    padding: 2px;
                    margin: 3px;

                }
                .to-from h4{
                    font-size: 14px;
                    margin: 3px;
                    text-align: left;
                }
                .to-from p{
                    font-size: 13px;
                    text-align: left;
                }
                .from{
                    padding-right: 5px;
                }
                .heading{
                    padding: 10px;
                }

                .invoice-no{
                        width: 80%;
                }
.gst-table table tr td{
        border-bottom: 1px solid #ccc;
        
}
.gst-table table tr th.hed{
    background-color: #D9EDF7;
    text-align: center;
    padding-left: 0px;
    border-right: 1px solid #fff;
    font-size: 13px;
}
.info th{
    font-size: 12px;
}
.gst-table-summ{
    padding: 10px;
    width: 65%;

}

.total-am{
    width: 33%;
}
.gst-summary{
    vertical-align: top;
}
.gst-totals td{
font-size: 13px;
}
.gst-totals td p{
    margin: 0px 0px 5px;
}
.gst-totals td h4{
    text-align: right;
}

.gst-tot h4{
   margin: 0px;
}


.col-3{
    margin-bottom: 10px;
}

/*.payment, .details{
        padding: 10px;
}*/

.payment > h4, .details > h4{
        text-align: left;
}

.payment > h4 span, .details > h4 span{
        border-bottom: 1px solid #555;
        padding-bottom: 5px;
}

.payment table, .details table{
        border: 1px solid #ccc;
}

.payment table tr td, .details table tr td{
    border: 1px solid #ccc;
}

.payment table tr th, .details table tr th{
    border: 1px solid #ccc;
}


.please-check{
    text-align: left;
    padding: 0px 10px;
}

.please-check h4{
    margin: 3px 0px;
}

.signature{
    padding: 0px 10px;
}

                /*Media Queries*/
@media only screen and (max-width: 600px) {
	
	a[class="btn"] { display:block!important; margin-bottom:10px!important; background-image:none!important; margin-right:0!important;padding: 5px 10px!important;}
}                
@media screen and (max-width: 400px) {

    .inner, .in-no-date,.from, .to{
    width: 100% !important;   
    display: block !important;            
    }
    .invoicefrom-address{
        width: 100% !important;
        display: block !important; 
    }
    .invoice-no{
        width: 100% !important;
        display: block !important; 
                }

 .to-from h2{
font-size: 16px !important;
 }
 .to-from h4 {font-size: 12px !important;}

.to-from p {
    font-size: 12px !important;

}

.gst-table table tr th.hed h4{
    font-size: 12px !important;
}

.total-am,.gst-summary{
display: block !important;
    width: 100% !important;
}

.info th{
    font-size: 11px !important;
}
.please-check h4{
font-size: 12px !important;
}
.please-check table td{
 display: block !important;
    width: 100% !important;   
}


}

</style>
    

