package com.tayyarah.insurance.model;

import java.io.Serializable;
import java.math.BigDecimal;

import com.tayyarah.common.gstconfig.model.InsurenceGstTax;

public class PriceDetail implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private BigDecimal apiBasePrice;
	private BigDecimal basePrice;
	private BigDecimal apiTaxes;
	private BigDecimal taxes;
	private BigDecimal apiTotalPrice;
	private BigDecimal totalPrice;
	private BigDecimal totalPayableAmount;
	private InsurenceGstTax insurenceGstTax;
	
	public BigDecimal getApiBasePrice() {
		return apiBasePrice;
	}
	public BigDecimal getBasePrice() {
		return basePrice;
	}
	public BigDecimal getApiTaxes() {
		return apiTaxes;
	}
	public BigDecimal getTaxes() {
		return taxes;
	}
	public BigDecimal getApiTotalPrice() {
		return apiTotalPrice;
	}
	public BigDecimal getTotalPrice() {
		return totalPrice;
	}
	public BigDecimal getTotalPayableAmount() {
		return totalPayableAmount;
	}
	public void setApiBasePrice(BigDecimal apiBasePrice) {
		this.apiBasePrice = apiBasePrice;
	}
	public void setBasePrice(BigDecimal basePrice) {
		this.basePrice = basePrice;
	}
	public void setApiTaxes(BigDecimal apiTaxes) {
		this.apiTaxes = apiTaxes;
	}
	public void setTaxes(BigDecimal taxes) {
		this.taxes = taxes;
	}
	public void setApiTotalPrice(BigDecimal apiTotalPrice) {
		this.apiTotalPrice = apiTotalPrice;
	}
	public void setTotalPrice(BigDecimal totalPrice) {
		this.totalPrice = totalPrice;
	}
	public void setTotalPayableAmount(BigDecimal totalPayableAmount) {
		this.totalPayableAmount = totalPayableAmount;
	}
	public InsurenceGstTax getInsurenceGstTax() {
		return insurenceGstTax;
	}
	public void setInsurenceGstTax(InsurenceGstTax insurenceGstTax) {
		this.insurenceGstTax = insurenceGstTax;
	}
}
