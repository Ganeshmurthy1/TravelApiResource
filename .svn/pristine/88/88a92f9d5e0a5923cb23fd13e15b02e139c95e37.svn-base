package com.tayyarah.bus.model;

import java.io.Serializable;

public class Seat implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private Integer zIndex;
	private String bookedBy;
	private Boolean ladiesSeat;
	private Boolean ac;
	private Boolean sleeper;
	private Integer width;
	private Boolean available;
	private Integer row;
	private Integer column;
	private Integer length;
	private String id;
	private SeatFare seatFare;
	
	public Integer getzIndex() {
		return zIndex;
	}
	public String getBookedBy() {
		return bookedBy;
	}
	public Boolean getLadiesSeat() {
		return ladiesSeat;
	}
	public Boolean getAc() {
		return ac;
	}
	public Boolean getSleeper() {
		return sleeper;
	}
	public Integer getWidth() {
		return width;
	}
	public Boolean getAvailable() {
		return available;
	}
	public Integer getRow() {
		return row;
	}
	public Integer getColumn() {
		return column;
	}
	public Integer getLength() {
		return length;
	}
	public String getId() {
		return id;
	}
	public SeatFare getSeatFare() {
		return seatFare;
	}
	public void setzIndex(Integer zIndex) {
		this.zIndex = zIndex;
	}
	public void setBookedBy(String bookedBy) {
		this.bookedBy = bookedBy;
	}
	public void setLadiesSeat(Boolean ladiesSeat) {
		this.ladiesSeat = ladiesSeat;
	}
	public void setAc(Boolean ac) {
		this.ac = ac;
	}
	public void setSleeper(Boolean sleeper) {
		this.sleeper = sleeper;
	}
	public void setWidth(Integer width) {
		this.width = width;
	}
	public void setAvailable(Boolean available) {
		this.available = available;
	}
	public void setRow(Integer row) {
		this.row = row;
	}
	public void setColumn(Integer column) {
		this.column = column;
	}
	public void setLength(Integer length) {
		this.length = length;
	}
	public void setId(String id) {
		this.id = id;
	}
	public void setSeatFare(SeatFare seatFare) {
		this.seatFare = seatFare;
	}
}
