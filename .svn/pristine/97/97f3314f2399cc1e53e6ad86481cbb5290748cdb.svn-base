					<!-- 0 parent himself, 1 parent to child, 2 child to parent , 3 child himself -->					
					 <h1 style="font-family: sans-serif;text-align:left;font-size: 18px;">Hi <b><span th:text="${receipientType}"></span></b>&nbsp;<span th:text="${receipient}"></span>,</h1>
					 
                <th:block th:switch="${actionType}">
  <div th:case="0">
     <!-- print object is not active -->
  </div>
   <div th:case="1">
     <b><span th:text="${parentType}"></span></b>&nbsp;<span th:text="${parent }"></span><span th:text="${action}"></span>&nbsp;<b><span th:text="${childType}"></span></b>&nbsp;<span th:text="${child }">.</span>
  </div>
   <div th:case="2">
     <b><span th:text="${childType}"></span></b>&nbsp;<span th:text="${child }"></span><span th:text="${action}"></span>&nbsp;<b><span th:text="${parentType}"></span></b>&nbsp;<span th:text="${parent }">.</span>
 
  </div>
  <div th:case="3">
    <b><span th:text="${childType}"></span></b>&nbsp;<span th:text="${child }"></span><span th:text="${action}">.</span>
              
  </div>
</th:block>
                
        
        
