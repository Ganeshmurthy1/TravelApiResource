                    text-align: left;
                }
                .text-right{
                    text-align: right;
                }
                .inner {
               text-align: left;            
                 }
                 .inner img{
                    padding: 10px;
                    
                 }
                 .in-no-date{
                    width: 60%        
                 }

                 .main-head{
                    background-color: #fff;
                    
                }


                .invoicefrom-address{
                    width: 60%;
                }
                .invoicefrom-address h4{
                        margin: 4px;
                }
                .invoice-no p{
                        font-size: 13px;
                        font-family: Georgia, Times, serif;
                        line-height: 0px;  
                        color: #fff;              
                }
                .invoice-no .h2no p{
                    font-size: 18px;
                }
                .to-from{
                    background-color: #f1f1f1;
                }
                .to-from h2{
                    background-color: #ccc;
                    padding: 2px;
                    margin: 3px;

                }
                .to-from h4{
                    font-size: 14px;
                    margin: 3px;
                    text-align: left;
                }
                .to-from p{
                    font-size: 13px;
                    text-align: left;
                }
                .from{
                    padding-right: 5px;
                }
                .heading{
                    padding: 10px;
                }

                .invoice-no{
                        width: 80%;
