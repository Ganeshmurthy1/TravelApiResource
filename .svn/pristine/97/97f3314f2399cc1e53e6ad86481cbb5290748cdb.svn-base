.gst-table table tr td{
        border-bottom: 1px solid #ccc;
        
}
.gst-table table tr th.hed{
    background-color: #D9EDF7;
    text-align: center;
    padding-left: 0px;
    border-right: 1px solid #fff;
    font-size: 13px;
}
.info th{
    font-size: 12px;
}
.gst-table-summ{
    padding: 10px;
    width: 65%;

}

.total-am{
    width: 33%;
}
.gst-summary{
    vertical-align: top;
}
.gst-totals td{
font-size: 13px;
}
.gst-totals td p{
